

<% if user_signed_in? %>
	<%= link_to "Show Courses", :courses %>	
	<%= link_to "Show Users", :users %>	
	<%= link_to "Register to Course", list_courses_path %>	
	<%= link_to "Show Registered Courses", list_registered_courses_path %>	


<% else %>
<h1>users</h1>
	<%= @users.each do |f|%>
		<h4><%= f.email%></h4>
		<h4><%= f.sign_in_count%></h4>
	<% end %>
	
<% end %>
